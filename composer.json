{
    "name": "rivalex/laravel-humanstxt",
    "description": "Simple humans.txt routes and views for Laravel 11",
    "keywords": [
        "rivalex",
        "Laravel",
        "humans.txt"
    ],
    "homepage": "https://github.com/rivalex/laravel-humanstxt",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Rivolta",
            "email": "info@rivalex.com",
            "homepage": "https://rivalex.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1.0",
        "orchestra/testbench-browser-kit": "^7.1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rivalex\\Humans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rivalex\\Humans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Rivalex\\Humans\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
