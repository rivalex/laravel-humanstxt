{
    "name": "rivalex/laravel7-humans",
    "description": "Simple humans.txt routes and views for Laravel 7|8",
    "keywords": [
        "rivalex",
        "Laravel 7",
        "humans.txt"
    ],
    "homepage": "https://github.com/rivalex/laravel7-humans",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Rivolta",
            "email": "info@rivalex.com",
            "homepage": "https://rivalex.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "orchestra/testbench-browser-kit": "^4.0",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rivalex\\Humans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rivalex\\Humans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Rivalex\\Humans\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
